{
  "$schema": "http://json.schemastore.org/prettierrc",
  "tabWidth": 2,
  "printWidth": 120,
  "useTabs": false,
  "singleQuote": true,
  "quoteProps": "consistent",
  "bracketSpacing": false,
  "bracketSameLine": true,
  "trailingComma": "none",
  "semi": false,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "parser": "flow"
      }
    }
  ]
}
